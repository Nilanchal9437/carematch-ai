{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/api/woner",
      "methods": ["POST"],
      "headers": {
        "Content-Type": "multipart/form-data"
      }
    }
  ],
  "functions": {
    "api/woner": {
      "memory": 3008,
      "maxDuration": 60
    }
  }
} 